
fun fibonacci(i: Int): Int {
    let 
        const pred: Bool = (i == 0 || i == 1)
    in
        if pred 
        then i 
        else fibonacci(i - 1) + fibonacci(i - 2)
}

fun main(): Unit {
    let 
        const n: Double = read() 
        const fibs: Double = fibonacci(n)
    in
        print(fibs)
}




